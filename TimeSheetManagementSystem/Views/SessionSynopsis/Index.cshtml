@model IEnumerable<TimeSheetManagementSystem.Models.SessionSynopsis>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.SessionSynopsisName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IsVisible)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpdatedBy)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SessionSynopsisName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsVisible)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedBy.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedBy.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SessionSynopsisId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SessionSynopsisId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SessionSynopsisId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
